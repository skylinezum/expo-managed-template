{
	"expo": {
		"name": "app",
		"slug": "app",
		"scheme": "app",
		"platforms": ["ios", "android", "web"],
		"version": "0.0.1",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"web": {
			"favicon": "./assets/images/favicon.png"
		}
	}
}
